# AWS Credentials (Ensure that these are set correctly in Jenkins credentials for security)
AWS_ACCESS_KEY_ID=<your-aws-access-key-id>
AWS_SECRET_ACCESS_KEY=<your-aws-secret-access-key>

# AWS Region
AWS_REGION=us-east-1

# ECR Repository URI
ECR_REPOSITORY_URI=123456789012.dkr.ecr.us-east-1.amazonaws.com/my-repo

# GitHub Repository URL
GITHUB_REPO_URL=https://github.com/user/repo.git

# Path to kubeconfig for accessing the EKS cluster
KUBE_CONFIG_PATH=/home/jenkins/.kube/config

# Kubernetes Deployment Manifest File Paths
FRONTEND_DEPLOYMENT_FILE=k8s/manifests/frontend-deployment.yaml
BACKEND_DEPLOYMENT_FILE=k8s/manifests/backend-deployment.yaml
